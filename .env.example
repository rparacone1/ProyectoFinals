# Server Configuration
PORT=8080

# MongoDB Database URL
# Format: mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>
MONGO_URL=

# Admin Credentials
ADMIN_EMAIL=
ADMIN_PASSWORD=

# Authentication Secrets
# Use strong random strings for production
JWT_SECRET=
COOKIE_SECRET=

# Gmail Configuration
# Email must be a valid Gmail account with 2FA enabled
GMAIL_ACCOUNT=
# App password generated from Google Account settings
GMAIL_APP_PASSWORD=

# Example values (DO NOT USE IN PRODUCTION):
# PORT=8080
# MONGO_URL=mongodb+srv://username:password@clusterXX.mongodb.net/database
# ADMIN_EMAIL=admin@example.com
# ADMIN_PASSWORD=strongPassword123
# JWT_SECRET=your_jwt_secret_key_here
# COOKIE_SECRET=your_cookie_secret_here
# GMAIL_ACCOUNT=your.email@gmail.com
# GMAIL_APP_PASSWORD=xxxx xxxx xxxx xxxx